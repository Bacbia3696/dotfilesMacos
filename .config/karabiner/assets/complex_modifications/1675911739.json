{
  "title": "Fn + Enter to Insert",
  "rules": [
    {
      "description": "Map Fn + Enter to Insert",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "insert"
            }
          ]
        }
      ]
    }
  ]
}